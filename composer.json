{
    "name": "dynamic/silverstripe-recipe-book",
    "description": "A recipe book for SilverStripe websites",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "sheadawson/silverstripe-linkable": "^2.0",
        "silverstripe/recipe-cms": "^4.0@dev",
        "silverstripe/lumberjack": "^2.0@dev",
        "symbiote/silverstripe-gridfieldextensions": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "theseer/phpdox": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\RecipeBook\\": "src/",
            "Dynamic\\RecipeBook\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
